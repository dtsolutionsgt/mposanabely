<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relwsmail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@layout/back"
    tools:context=".ComDrop">

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="150dp"
        android:layout_height="96dp"
        android:layout_below="@+id/textView1"
        android:layout_marginStart="50dp"
        android:layout_marginTop="-11dp"
        android:layout_marginEnd="57dp"
        android:layout_toStartOf="@+id/textView1"
        android:onClick="askActualize"
        android:src="@drawable/mantenimientos48" />

    <Button
        android:id="@+id/btnSenHand"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView34"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="28dp"
        android:layout_toStartOf="@+id/textView1"
        android:background="@layout/back"
        android:minWidth="64dp"
        android:minHeight="32dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Enviar Configuración"
        android:textSize="24sp"
        android:textStyle="bold" />

    <RelativeLayout
        android:id="@+id/relRoad"
        android:layout_width="match_parent"
        android:layout_height="48dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_centerVertical="true"
            android:background="#9A999E"></RelativeLayout>

        <TextView
            android:id="@+id/lblTit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView4"
            android:paddingLeft="5dp"
            android:text="Comunicación "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#1A8AC6" />

        <ImageView
            android:id="@+id/ImageView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="5dp"
            android:adjustViewBounds="true"
            android:src="@drawable/logo_panel" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:adjustViewBounds="true"
            android:padding="5dp"
            android:src="@drawable/comunicacion_blanco_gris" />
    </RelativeLayout>

    <TextView
        android:id="@+id/lblProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lblETipo"
        android:layout_centerHorizontal="true"
        android:paddingTop="5dp"
        android:paddingBottom="10dp"
        android:text="Archivo de parametros"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/lblCPago"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_below="@+id/btnRec"
        android:layout_alignParentRight="true"
        android:layout_marginTop="20dp"
        android:layout_marginRight="0dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:visibility="invisible" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:onClick="writeData"
        android:text="Button"
        android:visibility="invisible" />

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_above="@+id/relbotpan"
        android:layout_alignParentBottom="false"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="@layout/frame_rect">

        <TextView
            android:id="@+id/textView81"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="-1dp"
            android:padding="5dp"
            android:text="URL"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/txtWS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtRuta"
            android:layout_alignStart="@+id/txtEmp"
            android:layout_alignParentLeft="false"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:ems="10"
            android:enabled="true"
            android:nextFocusForward="@id/txtEmp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="5dp"
            android:layout_below="@+id/txtWS"
            android:layout_centerHorizontal="true"
            android:text="Medium Text"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="-1dp"
            android:padding="5dp"
            android:text="Empresa"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/txtEmp"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView4"
            android:ems="10"
            android:nextFocusLeft="@id/txtRuta"
            android:nextFocusDown="@id/txtWS"
            android:singleLine="true" />
    </RelativeLayout>

    <Button
        android:id="@+id/Button01"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/relativeLayout1"
        android:layout_alignParentRight="true"
        android:background="@layout/back"
        android:minWidth="64dp"
        android:minHeight="18dp"
        android:onClick="comManual"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="3dp"
        android:text="Manual"
        android:textSize="15sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/lblETipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relRoad"
        android:layout_alignLeft="@+id/relativeLayout1"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="-1dp"
        android:layout_marginTop="1dp"
        android:layout_marginRight="1dp"
        android:gravity="center_horizontal"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="Procesado" />

    <ProgressBar
        android:id="@+id/progressBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lblProd"
        android:layout_alignParentStart="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="-1dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="150dp"
        android:layout_height="96dp"
        android:layout_alignTop="@+id/imageView6"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="50dp"
        android:layout_marginBottom="3dp"
        android:layout_toStartOf="@+id/textView1"
        android:adjustViewBounds="true"
        android:clickable="true"
        android:onClick="askRec"
        android:src="@drawable/recibir_archivos" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="150dp"
        android:layout_height="95dp"
        android:layout_alignTop="@+id/progressBar2"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="0dp"
        android:layout_toEndOf="@+id/textView1"
        android:adjustViewBounds="true"
        android:clickable="true"
        android:onClick="askSend"
        android:src="@drawable/enviar_archivos" />

    <Button
        android:id="@+id/btnRec"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5"
        android:layout_alignEnd="@+id/imageView5"
        android:layout_marginStart="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="0dp"
        android:layout_toLeftOf="@+id/textView1"
        android:background="@layout/back"
        android:minWidth="64dp"
        android:minHeight="32dp"
        android:onClick="askRec"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Recibir"
        android:textSize="24sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnSend"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView6"
        android:layout_alignStart="@+id/imageView6"
        android:layout_alignEnd="@+id/imageView6"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_toRightOf="@+id/textView1"
        android:background="@layout/back"
        android:minWidth="64dp"
        android:minHeight="32dp"
        android:onClick="askSend"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Enviar "
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:visibility="invisible" />

    <RelativeLayout
        android:id="@+id/relbotpan"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="1dp"
        android:background="@layout/blue_strip"
        android:gravity="center">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:gravity="center_horizontal"
            android:paddingRight="10dp"
            android:text="DTSolutions"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#FFFFFF"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/txtVersion"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="87dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            android:layout_toStartOf="@+id/textView9"
            android:enabled="true"
            android:gravity="center"
            android:textColor="#FFFFFF"
            android:visibility="visible" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="84dp"
        android:layout_height="48dp"
        android:layout_above="@+id/Button01"
        android:layout_alignParentRight="true"
        android:adjustViewBounds="true"
        android:clickable="true"
        android:onClick="comManual"
        android:padding="5dp"
        android:src="@drawable/comunicacion_blanco_gris"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="116dp"
        android:layout_height="96dp"
        android:layout_below="@+id/textView1"
        android:layout_alignParentEnd="false"
        android:layout_marginStart="63dp"
        android:layout_marginTop="-11dp"
        android:layout_toEndOf="@+id/textView1"
        android:onClick="doFotos"
        android:src="@drawable/act_stock" />

    <Button
        android:id="@+id/btnSend2"
        android:layout_width="169dp"
        android:layout_height="61dp"
        android:layout_below="@+id/imageView46"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@+id/textView1"
        android:layout_toRightOf="@+id/textView1"
        android:background="@layout/back"
        android:minWidth="64dp"
        android:minHeight="32dp"
        android:onClick="doFotos"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Recibir Imágenes"
        android:textSize="24sp"
        android:textStyle="bold" />

</RelativeLayout>