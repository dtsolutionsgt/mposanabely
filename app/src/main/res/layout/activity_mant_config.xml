<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dtsgt.mant.MantConfig">

    <ImageView
        android:id="@+id/imgImg2"
        android:layout_width="64dp"
        android:layout_height="48dp"
        android:layout_below="@+id/relRoad"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="478dp"
        android:clickable="true"
        android:onClick="doSave"
        android:src="@drawable/icok" />

    <RelativeLayout
        android:id="@+id/relRoad"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#FFFFFF">

        <RelativeLayout
            android:id="@+id/relRoadbar"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_centerVertical="true"
            android:background="#9A999E"></RelativeLayout>

        <ImageView
            android:id="@+id/imgTitLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="5dp"
            android:adjustViewBounds="true"
            android:src="@drawable/logo_panel" />

        <ImageView
            android:id="@+id/imgRoadTit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/relRoadbar"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:adjustViewBounds="true"
            android:padding="5dp"
            android:src="@drawable/mantenimientos48" />

        <TextView
            android:id="@+id/lblTit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imgRoadTit"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:text="Configuración general"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#1A8AC6"
            android:textSize="24sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="262dp"
        android:layout_below="@id/imgImg2"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="45dp"
        android:layout_marginRight="10dp"
        android:layout_toStartOf="@+id/imgImg2"
        android:layout_toEndOf="@+id/imageView30"
        android:background="@drawable/frame_rect">

        <TextView
            android:id="@+id/lblCFact25"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/lblCFact24"
            android:layout_alignStart="@+id/lblCFact22"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignParentLeft="false"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="-5dp"
            android:background="@drawable/blue_strip"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="FEL"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="normal" />

        <CheckBox
            android:id="@+id/checkBox21"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/spinner16"
            android:layout_alignStart="@+id/spinner16"
            android:layout_alignEnd="@+id/spinner16"
            android:layout_alignParentBottom="false"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="0dp"
            android:layout_toRightOf="@id/textView2"
            android:clickable="true"
            android:enabled="true" />

        <TextView
            android:id="@+id/lblCFact24"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/lblCFact22"
            android:layout_alignStart="@+id/lblCFact22"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignParentLeft="false"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="0dp"
            android:background="@drawable/blue_strip"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="Formato impresión"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="normal" />

        <CheckBox
            android:id="@+id/checkBox10"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/checkBox8"
            android:layout_alignParentStart="false"
            android:layout_alignParentBottom="false"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="0dp"
            android:layout_toRightOf="@id/textView2"
            android:clickable="true"
            android:enabled="true" />

        <CheckBox
            android:id="@+id/checkBox23"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/lblCFact22"
            android:layout_alignParentStart="false"
            android:layout_alignParentBottom="false"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="0dp"
            android:layout_toRightOf="@id/textView2"
            android:clickable="true"
            android:enabled="true" />

        <TextView
            android:id="@+id/lblCFact22"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/lblCFact20"
            android:layout_alignStart="@+id/lblCFact"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/blue_strip"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="Usar biometrico"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="normal" />

        <CheckBox
            android:id="@+id/checkBox22"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/lblCFact20"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="10dp"
            android:layout_marginTop="0dp"
            android:layout_toEndOf="@+id/lblCFact20"
            android:clickable="true"
            android:enabled="true" />

        <CheckBox
            android:id="@+id/checkBox8"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="false"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="false"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="0dp"
            android:layout_toRightOf="@id/textView2"
            android:clickable="true"
            android:enabled="true" />

        <TextView
            android:id="@+id/lblCFact20"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/lblCFact19"
            android:layout_alignStart="@+id/lblCFact"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="0dp"
            android:background="@drawable/blue_strip"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="Imprimir factura"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/lblCFact19"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_below="@+id/textView2"
            android:layout_alignStart="@+id/lblCFact"
            android:layout_alignEnd="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="0dp"
            android:background="@drawable/blue_strip"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="Lista con imagenes"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="250dp"
            android:layout_height="32dp"
            android:layout_alignTop="@+id/checkBox8"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/blue_strip"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:text="Configuración centralizada"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#FFFFFF" />

        <TextView
            android:id="@+id/lblBU"
            android:layout_width="wrap_content"
            android:layout_height="10dp"
            android:layout_below="@+id/txt2"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="3dp"
            android:text="Small Text"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:visibility="invisible" />

        <Spinner
            android:id="@+id/spinner16"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_alignStart="@+id/checkBox23"
            android:layout_alignTop="@+id/lblCFact24"
            android:layout_alignEnd="@+id/checkBox23"
            android:layout_alignBottom="@+id/lblCFact24"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/spinner_style"
            android:focusable="true"
            android:focusableInTouchMode="true" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relbotpan"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/blue_strip">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:gravity="center_horizontal"
            android:paddingRight="10dp"
            android:text="DTSolutions"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#FFFFFF"
            android:textSize="15sp" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="64dp"
        android:layout_height="48dp"
        android:layout_below="@id/relRoad"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:clickable="true"
        android:onClick="doExit"
        android:src="@drawable/prev48" />


</RelativeLayout>